@import "../../wxParse/wxParse";
.title {
    font-weight: bold;
    font-size: 36rpx;
    width: 90%;
    height: 80rpx;
    line-height: 80rpx;
    margin: 0 auto;
    border-bottom: 1rpx #f3f3f3 solid;
}

.body {
    width: 90%;
    margin: 0 auto;
    line-height: 80rpx;
}

.box {
    width: 150rpx;
    height: 60rpx;
    color: white;
    text-align: center;
    line-height: 60rpx;
    background-color: #EC304C;
}

.box-1 {
    width: 40%;
    margin: 80rpx auto;
    position: fixed;
    bottom: 100rpx;
}

.box-2 {
    border-top-left-radius: 60rpx;
    border-bottom-left-radius: 60rpx;
}

.box-3 {
    border-top-right-radius: 60rpx;
    border-bottom-right-radius: 60rpx;
}

.bottom {
    width: 100%;
    height: 60rpx;
    background-color: black;
    color: white;
    font-size: 28rpx;
    position: fixed;
    bottom: 0;
}

.div-1 {
    width: 100%;
    height: 80rpx;
    background-color: black;
    position: fixed;
    bottom: 60rpx;
}

.m-left {
    margin-left: 35rpx;
}

.bgbox {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50rpx;
}

.color1 {
    background-color: white;
}

.color2 {
    background-color: #E8DFCB;
}

.color3 {
    background-color: #E8EDE5;
}

.color4 {
    background-color: #B6B6B6;
}

.color5 {
    background-color: #E8EDE5;
}

.body1 {
    width: 100%;
    height: 1400rpx;
}